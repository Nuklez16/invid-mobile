{
  "expo": {
    "name": "invid.au",
    "slug": "invid-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "invidmobiletemp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
"android": {
  "googleServicesFile": "./app/google-services.json",

  "softwareKeyboardLayoutMode": "resize",

  "adaptiveIcon": {
    "foregroundImage": "./assets/images/icon.png",
    "backgroundColor": "#000000"
  },

  "edgeToEdgeEnabled": true,
  "predictiveBackGestureEnabled": false,
  "package": "com.nukleuz.invidmobile"
},
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-notifications",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "49d5b7d8-723b-4cac-839f-1e21bc1a44f8"
      }
    },
    "owner": "nukleuz",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/49d5b7d8-723b-4cac-839f-1e21bc1a44f8",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    }
  }
}
